<% link_id=function(x){
    return x.replace(/[`~!@#$%^&*()_|+\-=?;:'",.<>\{\}\[\]\\\/]/gi, '').replace(/ /g,'');
} %>
<!--Add Monitor Window-->
<div class="modal dark medium fade" id="add_monitor" role="dialog" aria-labelledby="add_monitorLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <form class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
        <h4 class="modal-title" id="add_monitorLabel"><i class="fa fa-plus"></i> &nbsp; <span><%-lang.Add%></span> <%-lang.Monitor%> &nbsp; <small><%-lang['Monitor ID']%> : <t class="edit_id"></t></small></h4>
      </div>
      <div class="modal-body form-group-group-group">
          <div class="row">
          <div class="col-md-2 hidden-xs follow-list">
              <ul class="affix"></ul>
          </div>
        <div class="col-md-10">
        <div class="form-group-group grey" section id="monSectionIdentity">
          <h4><%-lang.Identity%></h4>
            <blockquote class="global_tip am_notice am_notice_new">
                <%-lang.IdentityText1%>
            </blockquote>
            <blockquote class="global_tip am_notice am_notice_edit">
                <%-lang.IdentityText2%>
            </blockquote>
            <div class="form-group">
            <label>
                <div><span><%-lang.Mode%></span></div>
                <div><select class="form-control" name="mode" selector="h_m">
                    <option value="stop"><%-lang.Disabled%></option>
                    <option value="idle"><%-lang.Idle%></option>
                    <option value="start"><%-lang['Watch Only']%></option>
                    <option value="record"><%-lang.Record%></option>
                </select></div>
            </label>
            </div>
              <div class="form-group">
                <label><div><span><%-lang['Monitor ID']%></span></div>
                    <div><input class="form-control" name="mid"></div>
                </label>
              </div>
              <div class="form-group">
                <label><div><span><%-lang.Name%></span></div>
                    <div><input class="form-control" name="name"></div>
                </label>
              </div>
              <div class="form-group">
                <label><div><span><%-lang['Retry Connection']%></span></div>
                    <div><input class="form-control" detail="fatal_max" placeholder="10"></div>
                </label>
              </div>
              <div class="form-group">
                <label><div><span><%-lang.Notes%></span></div>
                    <div><textarea class="form-control" detail="notes" placeholder="<%-lang.NotesPlacholder%>"></textarea></div>
                </label>
              </div>
            <div class="form-group">
            <label>
                <div><span><%-lang['Storage Location']%></span></div>
                <div><select class="form-control" detail="dir">
                    <option value="" selected><%-lang.Main%></option>
                    <% addStorage.forEach(function(v){ %>
                    <option value="<%-v.path%>"><%-v.name%></option>
                    <% })%>
                </select></div>
            </label>
            </div>
          </div>
<!--     START of Input     -->
          <div class="form-group-group forestgreen" section id="monSectionInput">
          <h4><%-lang.Input%></h4>
